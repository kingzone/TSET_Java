create table testsales(
"sales_tran_id", 
"visit_id", 
"tran_status_cd", 
"reported_as_dttm", 
"tran_type_cd", 
"mkb_cost_amt", 
"mkb_item_qty", 
"mkb_number_unique_items_qty", 
"mkb_rev_amt", 
"associate_party_id", 
"tran_start_dttm_dd", 
"tran_date", 
"tran_end_dttm_dd", 
"tran_end_hour", 
"tran_end_minute", 
"reward_cd" FROM "testsales";
)


CREATE MULTISET TABLE testsales ,NO FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO
     (
      SALES_TRAN_ID INTEGER TITLE 'SALES_TRAN_ID' NOT NULL,
      VISIT_ID INTEGER TITLE 'VISIT_ID',
      TRAN_STATUS_CD CHAR(1) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'T
    RAN_STATUS_CD',
      REPORTED_AS_DTTM TIMESTAMP(6) TITLE 'REPORTED_AS_DTTM',
      TRAN_TYPE_CD CHAR(1) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'TRA
    N_TYPE_CD',
      MKB_COST_AMT DECIMAL(18,4) TITLE 'MKB_COST_AMT',
      MKB_ITEM_QTY DECIMAL(18,4) TITLE 'MKB_ITEM_QTY',
      MKB_NUMBER_UNIQUE_ITEMS_QTY DECIMAL(18,4) TITLE 'MKB_NUMBER_UNIQUE_I
    TEMS_QTY',
      MKB_REV_AMT DECIMAL(18,4) TITLE 'MKB_REV_AMT',
      ASSOCIATE_PARTY_ID INTEGER TITLE 'ASSOCIATE_PARTY_ID',
      TRAN_START_DTTM_DD TIMESTAMP(6) TITLE 'TRAN_START_DTTM_DD',
      TRAN_DATE DATE FORMAT 'YYYY-MM-DD' TITLE 'TRAN_DATE',
      TRAN_END_DTTM_DD TIMESTAMP(6) TITLE 'TRAN_END_DTTM_DD',
      TRAN_END_HOUR INTEGER TITLE 'TRAN_END_HOUR',
      TRAN_END_MINUTE INTEGER TITLE 'TRAN_END_MINUTE',
      REWARD_CD VARCHAR(10) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'RE
    WARD_CD')
PRIMARY INDEX sat_nupi ( SALES_TRAN_ID );;

CREATE MULTISET TABLE "esca_<>?/'\\\,", DATABLOCKSIZE = 130048 BYTES, NO FALLBACK, NO BEFORE JOURNAL, NO AFTER JOURNAL, CHECKSUM = DEFAULT ("c1" FLOAT NULL, "c2" FLOAT NULL, "c3" TIMESTAMP FORMAT 'YYYY-MM-DDBHH:MI:SS.S(6)' NULL, "c4" VARCHAR(100) NULL, "c5" INTEGER NULL, "c6" VARCHAR(20) NULL, "c7" INTEGER NULL);

CREATE MULTISET TABLE t1 ,NO FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO
     (
      a1 CHAR(2) CHARACTER SET LATIN NOT CASESPECIFIC NOT NULL,
      b1 VARCHAR(16) CHARACTER SET LATIN NOT CASESPECIFIC NOT NULL,
      d1 INTEGER NOT NULL,
      e1 SMALLINT NOT NULL,
      f1 BIGINT NOT NULL,
      g1 BYTEINT NOT NULL,
      h1 FLOAT NOT NULL,
      i1 Double precision NOT NULL,
      j1 real NOT NULL,
      k1 numeric NOT NULL,
      l1 DECIMAL(18,2) NOT NULL,
      m1 TIMESTAMP(6) NOT NULL,
      n1 TIME(6) NOT NULL,
      o1 DATE FORMAT 'YY/MM/DD' NOT NULL,
      p1 PERIOD(DATE) NOT NULL,
      q1 INTERVAL YEAR(2) NOT NULL,
      r1 BYTE(1) NOT NULL,
      s1 VARBYTE(4) NOT NULL,
      t1 BLOB(2097088000) NOT NULL,
      u1 CLOB(2097088000) CHARACTER SET LATIN NOT NULL,
          v1 period(time) not null,
          w1 period(timestamp) not null,
      x1 INTERVAL YEAR(4) TO MONTH not null,
      y1 INTERVAL MONTH(4) not null,
      z1 INTERVAL DAY(4) not null,
      aa INTERVAL DAY(4) TO HOUR not null,
      ab INTERVAL DAY(4) TO MINUTE not null,
      ac INTERVAL DAY(4) TO SECOND(6) not null,
      ad INTERVAL HOUR(4) not null,
      ae INTERVAL HOUR(4) TO MINUTE not null,
      af INTERVAL HOUR(4) TO SECOND(6) not null,
      ag INTERVAL MINUTE(4) not null,
      ah INTERVAL MINUTE(4) TO SECOND(6) not null,
      ai INTERVAL SECOND(4,6) not null
         )
NO PRIMARY INDEX;

select * from dbc.tables where databasename='hadoop' order by createtimestamp;

CREATE MULTISET TABLE "testSales_115022", DATABLOCKSIZE = 130048 BYTES, NO FALLBACK, NO BEFORE JOURNAL, NO AFTER JOURNAL, CHECKSUM = DEFAULT ("c1" INTEGER NULL, "c2" CHAR(1) NULL, "c3" TIMESTAMP FORMAT 'YYYY-MM-DDBHH:MI:SS.S(6)' NULL, "c4" CHAR(1) NULL, "c5" DECIMAL(18, 4) NULL, "c6" DECIMAL(18, 4) NULL, "c7" DECIMAL(18, 4) NULL, "c8" DECIMAL(18, 4) NULL, "c9" INTEGER NULL, "c10" TIMESTAMP FORMAT 'YYYY-MM-DDBHH:MI:SS.S(6)' NULL, "c11" DATE NULL, "c12" TIMESTAMP FORMAT 'YYYY-MM-DDBHH:MI:SS.S(6)' NULL, "c13" INTEGER NULL, "c14" INTEGER NULL, "c15" VARCHAR(10) NULL, "c16" INTEGER NOT NULL, "TDIN_PARTID" INTEGER NULL) PRIMARY INDEX (TDIN_PARTID) PARTITION BY TDIN_PARTID;

CREATE MULTISET TABLE HADOOP.td_nopi ,NO FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO
     (
      tc1 INTEGER,
      tc2 FLOAT,
      tc3 VARCHAR(100) CHARACTER SET LATIN NOT CASESPECIFIC,
      tc4 INTEGER,
	  t1 INTEGER,
	  t2 INTEGER,
	  t3 INTEGER,
	  t4 INTEGER,
	  t5 INTEGER,
	  t6 INTEGER,
	  t7 INTEGER)
      no primary index;

CREATE MULTISET TABLE HADOOP.hourly_pv ,NO FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO
     (
      yomodo BIGINT,
      page_hour BIGINT,
      page_language VARCHAR(16) CHARACTER SET LATIN NOT CASESPECIFIC,
      page_name VARCHAR(2000) CHARACTER SET LATIN NOT CASESPECIFIC,
      page_view BIGINT,
      page_size BIGINT)
NO PRIMARY INDEX ;